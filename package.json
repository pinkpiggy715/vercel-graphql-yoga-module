  "main": "index.js",
  "name": "vercel-graphql-yoga-module",
  "private": true,
  "scripts": {
    "build": "npm run clean && tsc && babel src --out-dir dist  --extensions .ts && npm run copy-files",
    "clean": "rimraf coverage dist ",
    "copy-files": "copyfiles -u 1 src/**/*.graphql dist ",
    "dev": "concurrently \"npm run start\" \"npm run generate\"",
    "generate": "graphql-codegen-esm --config codegen.ts",
    "husky:commit-msg": "npx --no commitlint -- --edit",
    "husky:pre-commit": "npx --no lint-staged",
    "husky:pre-push": "npm run test",
    "serve": "node --experimental-modules --es-module-specifier-resolution=node dist /index.js",
    "start": "NODE_ENV=development nodemon src/index.ts",
    "test": "NODE_OPTIONS=--experimental-vm-modules NODE_ENV=test jest",
    "test:all": "npm run test --prefix client && npm run test --prefix server",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules NODE_ENV=test jest --coverage"
  },
  "sideEffects": false,
  "type": "module",
  "version": "1.0.0"
